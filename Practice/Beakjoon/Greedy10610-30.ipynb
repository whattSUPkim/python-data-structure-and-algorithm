    "n = list(input())\n",
    "n.sort(reverse = True)\n",
    "if n[-1] != '0':\n",
    "    result = -1\n",
    "else:\n",
    "    num = int(''.join(n))\n",
    "    if int(num) % 3 == 0:\n",
    "        result = int(num)\n",
    "    else:\n",
    "        result = -1\n",
    "\n",
    "print(result)"
