   "source": [
    "- 2번째 피보나치 = 바로 앞 두 피보나치 수의 합\n",
    "- 점화식\n",
    "    - n = (n-1) + (n-2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "id": "338c7e24",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "17\n",
      "1597\n"
     ]
    }
   ],
   "source": [
    "n = int(input())\n",
    "\n",
    "cache = [0, 1]\n",
    "for i in range(2, n + 1):\n",
    "    cache.append(cache[i - 2] + cache[i - 1])\n",
    "\n",
    "print(cache[n])"
   ]
  },
