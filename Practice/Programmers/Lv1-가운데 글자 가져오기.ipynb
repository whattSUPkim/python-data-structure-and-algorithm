    "def solution(s):\n",
    "    if len(s) % 2 == 0:\n",
    "        answer = s[len(s) // 2 - 1] + s[len(s) // 2]\n",
    "    else:\n",
    "        answer = s[len(s) // 2]\n",
    "    return answer"
   ]
