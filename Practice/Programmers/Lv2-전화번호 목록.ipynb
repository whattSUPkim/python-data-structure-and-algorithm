    "def solution(phone_book):\n",
    "    answer = True\n",
    "    sorted_pb = sorted(phone_book)\n",
    "    \n",
    "    for i in range(1, len(sorted_pb)):\n",
    "        if sorted_pb[i].startswith(sorted_pb[i - 1]):\n",
    "            return False\n",
    "\n",
    "    return answer"
   ]
