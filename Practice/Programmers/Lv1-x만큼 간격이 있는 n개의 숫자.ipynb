    "def solution(x, n):\n",
    "    if x == 0:\n",
    "        return [0 for _ in range(n)]\n",
    "    \n",
    "    answer = []\n",
    "    if x > 0:\n",
    "        for i in range(x, x*n + 1, x):\n",
    "            answer.append(i)\n",
    "    else:\n",
    "        for i in range(x, x*n - 1, x):\n",
    "            answer.append(i)\n",
    "            \n",
    "    return answer"
   ]
