    "def solution(arr):\n",
    "    answer = ['wonsup']\n",
    "    for i in arr:\n",
    "        if i != answer[-1]:\n",
    "            answer.append(i)\n",
    "    answer.pop(0)\n",
    "    \n",
    "    return answer"
